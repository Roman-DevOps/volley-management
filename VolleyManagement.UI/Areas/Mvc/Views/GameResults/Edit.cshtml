@model VolleyManagement.UI.Areas.Mvc.ViewModels.GameResults.GameResultViewModel

@{
    ViewBag.Title = Resources.GameResultViews.EditTitle;
}

<h2>@Resources.GameResultViews.EditTitle</h2>

@using (Html.BeginForm("Edit", "GameResults", FormMethod.Post, new { id = "createForm", name = "createForm" }))
{
    <hr />

    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    @Html.Partial("_CreateOrEdit", Model)

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="@Resources.GameResultViews.Edit" class="btn btn-default" />
        </div>
    </div>

}

<script src="~/Scripts/GameResults/ResultsValidate.js" type="text/javascript"></script>


